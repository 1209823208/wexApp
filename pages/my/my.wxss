.page-my .header {
  width: 750rpx;
  height: 573rpx;
  position: relative;
  text-align: center;
}

.page-my .header .background-img {
  width: 100%;
  height: 100%;
}

.page-my .header-img {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -60rpx;
  margin-left: -60rpx;
  width: 120rpx;
  height: 120rpx;
}

.page-my .header-img image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.header-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: absolute;
  bottom: 50rpx;
  width: 750rpx;
}

.header-bottom .about, .header-bottom .like-nums {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header-bottom .about image {
  width: 34rpx;
  height: 34rpx;
}

.like-content {
  background: gainsboro;
}

.like-content .all-like-content {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 0rpx 2%;
  justify-content: space-between;
}
.text{
  display: block;
  text-align: center
}
.like-content .all-like-content .like-item {
  width: 48%;
  background: #fff;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
}

.item-header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}

.rdu {
  border-radius: 50%;
}

.like-img {
  width: 211rpx;
  height: 211rpx;
}
